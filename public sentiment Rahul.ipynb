    "def get_tweets_Sentiment(sentiment_score):\n",
    "    if sentiment_score == 'negative':\n",
    "        return '-1'\n",
    "    elif sentiment_score == 'neutral':\n",
    "        return '0'\n",
    "    else:\n",
    "        return '1'\n",
    "    \n",
    "df['Sentiment_Score'] = df['senti'].apply(get_tweets_Sentiment)"
   ]
