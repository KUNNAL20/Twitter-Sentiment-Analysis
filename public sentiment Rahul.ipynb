    "import re\n",
    "\n",
    "\n",
    "def cleanTxt(txt):\n",
    "    \n",
    "    txt = re.sub(r'@[A-Za-z0-9_:]+', '' , txt)\n",
    "    \n",
    "    txt = re.sub(r'#', '' , txt)\n",
    "    \n",
    "    txt = re.sub(r'RT[\\s]+','',txt)\n",
    "    \n",
    "    txt = re.sub(r'https?:\\/\\/\\S+','',txt)\n",
    "    \n",
    "    txt = re.sub(r'\\d+', '', txt)\n",
    "    return txt\n",
    "\n",
    "df['text'] = df[\"text\"].apply(cleanTxt)"
   ]
